{
  "name": "devmeetings-eventbrite-dashboard",
  "version": "0.0.0",
  "license": "MIT",
  "author": "Lukasz Szewczak",
  "scripts": {
    "ng": "ng",
    "start:ng": "ng serve",
    "build": "ng build",
    "test:ng": "ng test",
    "lint:ng": "ng lint",
    "e2e": "ng e2e",
    "start": "node app.js",
    "debug": "node %NODE_DEBUG_OPTION% app.js",
    "start:dev": "ng build && set NODE_ENV=development&& nodemon app.js",
    "test": "nyc mocha test/**/*.spec.js --reporter spec",
    "test:dev": "set NODE_ENV=development&&nyc mocha test/**/*.spec.js --reporter spec",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "postinstall": "ng build --aot -prod"
  },
  "nyc": {
    "include": [
      "server/routes/**/*.js"
    ],
    "exclude": [
      "node_modules/"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "private": true,
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/cli": "1.1.1",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "debug": "^2.6.8",
    "express": "^4.15.3",
    "express-session": "^1.15.3",
    "joi": "^10.5.2",
    "moment": "^2.18.1",
    "morgan": "^1.8.2",
    "passport": "^0.3.2",
    "passport-google-oauth20": "^1.0.0",
    "request": "^2.81.0",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4",
    "typescript": "~2.3.3"
  },
  "devDependencies": {
    "@angular/language-service": "^4.0.0",
    "@types/jasmine": "2.5.45",
    "@types/node": "~6.0.60",
    "chai": "^4.0.2",
    "codelyzer": "~3.0.1",
    "coveralls": "^2.13.1",
    "dotenv": "^4.0.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mocha": "^3.4.2",
    "node-mocks-http": "^1.6.2",
    "nodemon": "^1.11.0",
    "nyc": "^10.3.2",
    "protractor": "~5.1.2",
    "sinon": "^2.3.4",
    "sinon-chai": "^2.11.0",
    "ts-node": "~3.0.4",
    "tslint": "~5.3.2"
  }
}
